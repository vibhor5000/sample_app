<%= render 'common_templates/company_name' %>

<h1>List User</h1>



<table style="border: 2px solid;">
  <th> First Name </th>
  <th> Last Name </th>
  <th> Age </th>
  <th> City </th>
  <th> Gender </th>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.age %></td>
      <td><%= user.city %></td>
      <td><%= user.gender_string %></td>

    </tr>
  <% end %>
</table>
